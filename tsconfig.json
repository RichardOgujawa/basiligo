{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"removeComments": true,
		"noEmitOnError": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"skipLibCheck": true,
		"jsx": "react-jsx"
	},
	"extends": "expo/tsconfig.base"
}
